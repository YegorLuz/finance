{
  "name": "address-book",
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently \"npm run start-server\" \"npm run eslint\" \"npm run start-dev\"",
    "start-dev": "cross-env NODE_ENV=dev webpack-dev-server --hot --inline",
    "start-prod": "cross-env NODE_ENV=prod webpack-dev-server --hot --inline",
    "build": "concurrently \"npm run eslint\" \"npm run build-prod\"",
    "build-prod": "cross-env NODE_ENV=prod webpack --progress",
    "eslint": "eslint src/**/*.js",
    "start-server": "json-server --watch server/db.json"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.7.22",
    "@types/react-datepicker": "^2.0.3",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "html-react-parser": "^0.6.1",
    "immutable": "^4.0.0-rc.12",
    "json-server": "^0.14.2",
    "moment": "^2.24.0",
    "react": "^16.7.0",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "typescript": "^3.2.4"
  }
}
